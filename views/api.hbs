{{#if generar.length}}

<div  class="table-responsive">
    <table action="/api" class="table table-dark">
    <tr style="color: rgb(66, 61, 135);">
        <th>Id</th>
        <th>Nombre</th>
        <th>Precio</th>
        <th>Foto</th>
    </tr>
    {{#each .}}
        <tr>
        <td>{{id}}</td>    
        <td>{{nombre}}</td>
        <td>{{precio}}</td>
        <td>{{foto}}</td>
        </tr>
    {{/each}}
    </table>

    
</div>
{{else}}
<h3 class="alert alert-danger">No se encontraron datos</h3>
{{/if}}
    <button
        onclick={window.location.replace("/formulario")}
        type="button"
        class="btn btn-success"
    >Volver</button>